[user]
	email = huangmaosen.y18@bytedance.com
	name = huangmaosen
[core]
	pager = diff-so-fancy | less --tabs=4 -RFX
	excludesfile = /Users/huangmaosen/.gitignore_global
	quotepath = false
	editor = nvim
[color]
	ui = true
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[color "diff"]
	meta = yellow
	frag = magenta bold
	commit = yellow bold
	old = red bold
	new = green bold
	whitespace = red reverse
[github]
	user = methor
[credential]
	helper = osxkeychain
[hub]
	protocol = ssh
[url "git@code.byted.org:"]
	insteadOf = https://code.byted.org/
[url "ssh://huangmaosen.y18@git.byted.org:29418"]
	insteadOf = https://git.byted.org
[url "gitr"]
	insteadOf = git://git.byted.org
[init]
	templatedir = ~/repos/git_templates
[gui]
	encoding = utf-8
[i18n]
	commitencoding = utf-8
	logOutputEncoding = utf8
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
